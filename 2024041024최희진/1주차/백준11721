#include<iostream>
#include<string>
#include<sstream>
using std::cout;
using std::cin;
using std::string;
int main() {
	string str1;
	cin >> str1;
	int len = str1.length();
	int i = 0;
	while (i < len) {
		for (int j = 0; j < 10; j++) {
			cout << str1[i + j];
			if (i+j==len-1)return 0;
		}
		cout << "\n";
		i=i + 10;
	}

	return 0;
}
